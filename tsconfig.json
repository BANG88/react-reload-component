{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "node",
		"lib": [
			"dom",
			"es2016"
		],
		"target": "esnext",
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": false,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": true,
		"allowSyntheticDefaultImports": true,
		"importHelpers": true,
		"jsx": "react"
	},
	"files": [
		"index.tsx",
		"test.tsx"
	]
}
